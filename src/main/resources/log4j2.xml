<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="Jetty" >
   <Appenders>
    <Console name="console" target="SYSTEM_ERR">
      <PatternLayout>
        <Pattern>[%d{yyyy-MM-dd HH:mm:ss,SSS} %5p %c{1},%t:%L]%n===%m%n</Pattern>
      </PatternLayout>
    </Console>
  </Appenders>

  <Loggers>
    <Root level="info">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>

</Configuration>

